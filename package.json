{
  "name": "@octocloud/core",
  "version": "1.0.22",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "format:check": "prettier --check \"**/*.{ts,js,css,json,md}\"",
    "format": "prettier --write \"**/*.{ts,js,css,json,md}\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "TZ=Europe/Prague jest --verbose",
    "test:coverage": "TZ=Europe/Prague jest --verbose --coverage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@octocloud/generators": "github:octotravel/octo-generator",
    "@types/jest": "^29.2.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "esbuild": "^0.15.12",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.5",
    "tsup": "^6.3.0",
    "unbuild": "^0.9.4"
  },
  "dependencies": {
    "@dinero.js/currencies": "2.0.0-alpha.8",
    "@octocloud/types": "^1.5.11",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dinero.js": "^2.0.0-alpha.14",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "uuid": "^9.0.0"
  }
}
